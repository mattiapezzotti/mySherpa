<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/fragment_explore">

    <androidx.fragment.app.FragmentContainerView
        android:name="it.unimib.camminatori.mysherpa.ui.fragment.Explore_Map_Fragment"
        android:id="@+id/fragment_map_explore"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <androidx.fragment.app.FragmentContainerView
        android:name="it.unimib.camminatori.mysherpa.ui.fragment.SearchBar_Fragment"
        android:id="@+id/fragment_search_bar"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <androidx.fragment.app.FragmentContainerView
        android:name="it.unimib.camminatori.mysherpa.ui.fragment.Explore_Card_fragment"
        android:id="@+id/fragment_explorecard"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</FrameLayout>